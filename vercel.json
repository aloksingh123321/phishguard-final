{
    "functions": {
        "api/index.py": {
            "includeFiles": "backend/**",
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}